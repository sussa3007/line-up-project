<?xml version="1.0"?>
<thlogic>
    <attr sel="#back"  th:onclick="'location.href=\'' + @{${backUrl}} + '\''" />
    <attr sel="#modifyPost"  th:onclick="'location.href=\'' + @{'/posts/' + ${post.id}+'/modify'} + '\''" />
    <attr sel="#deletePost"  th:onclick="'location.href=\'' + @{'/posts/' + ${post.id} +'/delete/check'} + '\''" />
    <attr sel="#csrf" th:value="${_csrf.token}" th:name="${_csrf.parameterName}"/>


</thlogic>
